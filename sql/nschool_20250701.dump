CREATE DEFINER=`root`@`localhost` PROCEDURE `getSelectList`(IN dbNm text, _srcObjID int, _objectID int, _accountID int, _viewID int, _categoryNm text)
BEGIN
  
  select name into @sourceName from dframe.__object where id = _srcObjID;
  
  set @q1 = 'select 0 where  0=1';
  if _viewID = 0 then
     set @q_dic = '( select cd.toString, cd.ordinaryWord from dframe.__caption_dic cd inner join dframe.__systemini si on si.language=cd.language) ';
     if @sourceName = '__role' then
        set @q1 = concat('SELECT "role" AS catName ,IFNULL(DIC.toString,R.name) AS title, id AS value, id AS seq from dframe.__role ',
                         ' AS R left join ', @q_dic , ' AS DIC on R.name = DIC.ordinaryWord '
                          ' ORDER BY catName,seq');
     elseif @sourceName = '__base_value' or _categoryNm > '' and _srcObjID=0 then
        set @q1 = concat('SELECT "" AS catName , "" AS title, "" AS value , 0 AS seq UNION 
                          SELECT V.catName, IFNULL(DIC.toString,V.title), V.value , V.seq FROM dframe.__base_value ',
                         ' AS V left join ', @q_dic , ' AS DIC on V.title = DIC.ordinaryWord '
                         ' WHERE (catName="', _categoryNm, '")');
	elseif @sourceName = '__object' then
        set @q1 = concat('SELECT "" AS catName , "" AS title, "" AS value , 0 AS seq, "" AS dummy UNION
         SELECT "", IFNULL(DIC.toString, s.title) AS title, s.id, s.id, s.name  FROM ', dbNm , '.' , @sourceName , ' AS s ',         
         '  left join ', @q_dic , ' AS DIC on s.title = DIC.ordinaryWord '
         ' where runningType in (0,3) order by seq');
	elseif @sourceName = '__object_field' then
        set @q1 = concat('SELECT "" AS catName , "" AS title, "" AS value , 0 AS seq UNION
                          SELECT "", IFNULL(DIC.toString, f.title), f.id , f.seq  FROM  dframe.__object_field  f 
                           inner join __object_field_rw  r  on r.objectFieldId = f.ID AND r.isVisible=True ' , 
                          ' inner join __account a  on a.ID =', _accountID, ' and r.roleID = a.roleID ' , 
                         '  left join ', @q_dic , ' AS DIC on f.title = DIC.ordinaryWord '
                          ' where  f.objectId =' , _objectID , ' order by seq');
     elseif @sourceName > '' then
        set @q1 = concat('SELECT "" AS catName , "" AS title, "" AS value , 0 AS seq, "" AS dummy UNION
         SELECT "", IFNULL(DIC.toString, s.name) AS title, s.id, s.id, s.name  FROM ', dbNm , '.' , @sourceName , ' AS s ',         
         '  left join ', @q_dic , ' AS DIC on s.name = DIC.ordinaryWord '
         ' order by seq');
     end if;
     
      
else
    set @q1 = concat('SELECT "" AS `title` , "" AS `value`, 0 as checked ,0 AS seq UNION ',

           ' SELECT IFNULL(DIC.toString,f.title) AS `title`, f.name AS`value`, case when v.id is null then 0 else 1 end  checked , f.seq ' ,
           ' FROM  __object_field f left join __list_view_field v on f.id = v.objectFieldID and v.listViewID=', _viewID ,
           ' inner join __object_field_rw rw on f.id = rw.objectFieldID ',
           ' inner join __account a on a.roleID = rw.roleID and a.id =', _accountID,
        
           ' left join ',
           '( select cd.toString, cd.ordinaryWord from __caption_dic cd inner join __systemini si on si.language=cd.language) ',
           ' AS DIC on f.title = DIC.ordinaryWord ',
           ' WHERE f.objectID=', _objectID,  ' and rw.isVisible=1 ',
           ' ORDER BY seq');

  end if;
  
  
  if _viewID = 999 then
   select @sourceName, @q1;
  end if;
  
  
  prepare stmt1 from @q1;
  execute stmt1;
  deallocate prepare stmt1;
END ;;
DELIMITER ;
