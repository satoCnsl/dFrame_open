CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryHealth`(_id int, scope text)
BEGIN
  DECLARE done INT DEFAULT FALSE;
  DECLARE _cause text;
  DECLARE _absent text;
  
  DECLARE cur1 CURSOR FOR 
   select distinct 
         concat(c.name, '(', m.cause,'縲',m.medCondition, ')' )  as cause
        from nschool.infirmarymemo m inner join nschool.child c on m.childID = c.id
                                     inner join nschool.classdiary d on d.date = m.date
                                                                    and d.id = _id;
  
  DECLARE cur2 CURSOR FOR 
   select distinct 
         concat(c.name, '(', a.cause, '縲', a.description,')' )   as absent
        from nschool.attending a inner join nschool.child c on a.childID = c.id
                                     inner join nschool.classdiary d on d.date = a.date
                                                                    and d.id = _id;
 
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE; 
  set @cause = '';
  OPEN cur1;
  read_loop1: LOOP
     FETCH cur1 INTO _cause;
     if _cause != 'NULL' and done = false then
        set @cause = concat( @cause, '縲', _cause); 
	 end if;
     
     IF done THEN
       LEAVE read_loop1;
     END IF;
	
  END LOOP;  
  CLOSE cur1;
  
  set done = false;
  set @absent = '';
  OPEN cur2;
  read_loop2: LOOP
     FETCH cur2 INTO _absent;
     if _absent != 'NULL'  and done = false then
		set @absent = concat( @absent, '縲', _absent);  
	 end if;
   
     IF done THEN
       LEAVE read_loop2;
     END IF;
  END LOOP;  
  CLOSE cur2;
    
  select concat('<p>逞豌励こ繧ｬ縺ｮ迥ｶ豕</p>',@cause) as fld
  union
  select concat('<p>逞谺縺ｨ縺昴ｮ逅逕ｱ</p>',@absent) ;
END ;;
DELIMITER ;
