     max(if(day = 11 , status, 'ｼ' )) as fld13,
     max(if(day = 12 , status, 'ｼ' )) as fld14,
     max(if(day = 13 , status, 'ｼ' )) as fld15,
     max(if(day = 14 , status, 'ｼ' )) as fld16,
