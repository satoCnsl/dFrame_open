CREATE DEFINER=`root`@`localhost` PROCEDURE `setMedication`(_id_accp int)
BEGIN
  select startingDate, uneffectiveDate into @date, @upto from nschool.accpmedicine where id = _id_accp;
  
  while datediff(@upto, @date) >= 0 do
     insert into medication (accpmedicineID, dose, scheduledDate) 
		select _id_accp, `dose`, @date from nschool.accpmedicine where id = _id_accp;	
        
	 set @date = date_add(@date, interval 1 day );
     
   end while;
 
END ;;
