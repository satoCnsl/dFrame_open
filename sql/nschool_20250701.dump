CREATE DEFINER=`root`@`localhost` PROCEDURE `putOffsetList`(in _objectID int, _viewID int)
BEGIN
  
   drop temporary table if exists temp_list_field_name;  
   create temporary table temp_list_field_name (
      offset_value int auto_increment,
      title text ,
      PRIMARY KEY (`offset_value`)
);
    
    select if(isnull(op.name), o.name, op.name) into @objNm 
                            from dframe.__object o
                       left join dframe.__object op on op.id = o.bulkSourceObjectID
where o.id  = _objectID;

    
    set @schoolAge = MOD(ROUND(_viewID/10),10);
    set @date = date_sub(curdate(), INTERVAL ROUND((100000 - _viewID)/100) day);
    set @wh = concat(" nschool.schoolAge(child_childID.birthday,'", @date ,"') = ", @schoolAge);

    set @q = concat("insert into temp_list_field_name (title) 
              select child_childID.name" , " from nschool.child as child_childID"
             " where ", @wh, " order by child_childID.birthday" );  
             

    prepare stmt from @q;
    execute stmt;
    deallocate prepare stmt;
END ;;
