CREATE DEFINER=`root`@`localhost` PROCEDURE `getSystemFieldCaption`()
BEGIN
  DECLARE done INT DEFAULT FALSE;
  DECLARE _cap text(200);
  DECLARE _toS text(200);
  DECLARE _sql text(1000);
  DECLARE cur CURSOR FOR
    select distinct bv.title  as capton, IFNULL(dic.toString, cd.ordinaryWord) as toString 
    from dframe.__caption_dic cd
 inner join  dframe.__base_value bv on bv.title = cd.ordinaryWord and bv.catName='fieldCaption'
         left join
        ( select toString, ordinaryWord from __caption_dic cd inner join dframe.__systemini  si on si.language = cd.language) dic
                                       on dic.ordinaryWord = cd.ordinaryWord ;
        
 DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE; 
  set _sql = 'SELECT ';
  OPEN cur;
  read_loop: LOOP
    FETCH cur INTO _cap, _toS;  
    IF done THEN
       LEAVE read_loop;
    END IF; 
    set _sql = concat(_sql, '"',_toS, '"',' AS ', _cap, ' ,');
    select _sql,_cap;
  END LOOP;  
  CLOSE cur;
  set done = FALSE;
  set @q = concat(_sql, ' 0 as dummy' );
  
  prepare stmt from @q;
  execute stmt;
  deallocate prepare stmt;
  
END ;;
DELIMITER ;
