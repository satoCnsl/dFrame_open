     union  select  CONCAT('<p>(蜊亥ｾ)</p><p>',@actPlanPM)
     union  select  CONCAT('<p>豢ｻ蜍輔ｮ蜀螳ｹｼ</p>',@activities)
     union  select  CONCAT('<p>諢滓Φ繝ｻ蜿咲怐ｼ</p>',@reflection) ;
   end if;
END ;;
