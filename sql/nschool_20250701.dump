     max(if(day = 26 , status, 'ｼ' )) as fld28,
     max(if(day = 27 , status, 'ｼ' )) as fld29,
     max(if(day = 28 , status, 'ｼ' )) as fld30,
     max(if(@numOfDays < 29, 'ｼ', if(day = 29, status, 'ｼ' )))  as fld31,
