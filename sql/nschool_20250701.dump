     max(case when day = 8  then status else '' end) as d8,
     max(case when day = 9  then status else '' end) as d9,
     max(case when day = 10  then status else '' end) as d10,
     max(case when day = 11  then status else '' end) as d11,
     max(case when day = 12  then status else '' end) as d12,
     max(case when day = 13  then status else '' end) as d13,
