CREATE DEFINER=`root`@`localhost` PROCEDURE `getWidthVal`(_id int, _colNo int)
BEGIN
   select 
     case 
       when _colNo = 1 then width_1stCol
       when _colNo = 2 then width_2ndCol
       when _colNo > 2 and _colNo < (numOfCols - 1)then width_midCol
       when _colNo = (numOfCols - 1) then width_1stTailCol
       else width_2ndTailCol
 end width 
   from dframe.__report_schema
   where id = _id;
END ;;
