   UNION

   select
     max(fld1) as fld1,
     name    as fld2,
     max(if(day =  1 , status, 'ｼ' )) as fld3,
     max(if(day =  2 , status, 'ｼ' )) as fld4,
     max(if(day =  3 , status, 'ｼ' )) as fld5,
