CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryBODY`(_id int, scope text)
BEGIN
   select d.note, d.activities, d.actPlanAM, d.actPlanPM,
          d.classID, d.reflection
     into  @note,  @activities,  @actPlanAM,  @actPlanPM,
           @classID, @reflection
