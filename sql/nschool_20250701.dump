CREATE DEFINER=`root`@`localhost` PROCEDURE `getRprtEntity`(_reportID int, _entityID int, _scope text)
BEGIN
select 0;
  select o.dbName, o.name, ff.name  , fs.name, fs.dataType
    into @dbNm,    @objNm, @ffNm    , @fsNm,   @fsDtyp 
    from dframe_nschool.__report r
    inner join dframe_nschool.__object o        on o.id = r.objectID
    inner join dframe_nschool.__object_field ff on ff.id = r.foreignKeyFieldID
    inner join dframe_nschool.__object_field fs on fs.id = r.scopeRelFieldID                                                 
    where r.id = _reportID;
    
    set @ffVal = '';
    set @fsVal = '';
    set @sql1 = concat('SELECT ', @ffNm, ',', @fsNm, ' INTO  @ffVal, @fsVal  FROM ', @dbNm, '.', @objNm, ' WHERE id=', _entityID);
          
    
    select 1,@sql1;
    
    prepare stmt1 from @sql1;
    execute stmt1;
    deallocate prepare stmt1;
    
    set @ffConstraint = concat( @ffNm, '=', @ffVal);
  
    set @fsConstraint = ' 1= 1 ';
    if _scope != 'id' and _scope != '' then
      set @sql2  = concat( "select dframe.getConstraint('", _scope,"','", @fsNm, "','", @fsVal, "') into @fsConstraint");      
    select 2, @sql2;
      prepare stmt2 from @sql2;
      execute stmt2;
      deallocate prepare stmt2;
      
    end if; 
  
    set @sql3 = concat('SELECT id  FROM ', @dbNm, '.', @objNm, ' WHERE ', @fsConstraint );
        
    
    select 3, @sql3;
    
    
    
    prepare stmt3 from @sql3;
    execute stmt3;
    deallocate prepare stmt3;  
 
END ;;
DELIMITER ;
