  if(@numOfDays < 30, 'ｼ',if(sum.fld32=49995,'ｼ',fld32)) as fld32,
  if(@numOfDays < 31, 'ｼ',if(sum.fld33=49995,'ｼ',fld33)) as fld33,
  fld34,fld35,fld36
from (
    select
     '**' as fld1,
     if(line = '蜃ｺ', '蜃ｺ蟶ｭ閠謨ｰ', '谺蟶ｭ閠謨ｰ')  as fld2,
     sum(if(day!= 1,0,if(status='H',9999,if(line=status, 1, 0)))) as fld3,
     sum(if(day!= 2,0,if(status='H',9999,if(line=status, 1, 0)))) as fld4,
     sum(if(day!= 3,0,if(status='H',9999,if(line=status, 1, 0)))) as fld5,
     sum(if(day!= 4,0,if(status='H',9999,if(line=status, 1, 0)))) as fld6,
     sum(if(day!= 5,0,if(status='H',9999,if(line=status, 1, 0)))) as fld7,
     sum(if(day!= 6,0,if(status='H',9999,if(line=status, 1, 0)))) as fld8,
     sum(if(day!= 7,0,if(status='H',9999,if(line=status, 1, 0)))) as fld9,
     sum(if(day!= 8,0,if(status='H',9999,if(line=status, 1, 0)))) as fld10,
     sum(if(day!= 9,0,if(status='H',9999,if(line=status, 1, 0)))) as fld11,
     sum(if(day!=10,0,if(status='H',9999,if(line=status, 1, 0)))) as fld12,
     sum(if(day!=11,0,if(status='H',9999,if(line=status, 1, 0)))) as fld13,
     sum(if(day!=12,0,if(status='H',9999,if(line=status, 1, 0)))) as fld14,
     sum(if(day!=13,0,if(status='H',9999,if(line=status, 1, 0)))) as fld15,
     sum(if(day!=14,0,if(status='H',9999,if(line=status, 1, 0)))) as fld16,
     sum(if(day!=15,0,if(status='H',9999,if(line=status, 1, 0)))) as fld17,
     sum(if(day!=16,0,if(status='H',9999,if(line=status, 1, 0)))) as fld18,
     sum(if(day!=17,0,if(status='H',9999,if(line=status, 1, 0)))) as fld19,
     sum(if(day!=18,0,if(status='H',9999,if(line=status, 1, 0)))) as fld20,
     sum(if(day!=19,0,if(status='H',9999,if(line=status, 1, 0)))) as fld21,
     sum(if(day!=20,0,if(status='H',9999,if(line=status, 1, 0)))) as fld22,
     sum(if(day!=21,0,if(status='H',9999,if(line=status, 1, 0)))) as fld23,
     sum(if(day!=22,0,if(status='H',9999,if(line=status, 1, 0)))) as fld24,
     sum(if(day!=23,0,if(status='H',9999,if(line=status, 1, 0)))) as fld25,
     sum(if(day!=24,0,if(status='H',9999,if(line=status, 1, 0)))) as fld26,
     sum(if(day!=25,0,if(status='H',9999,if(line=status, 1, 0)))) as fld27,
     sum(if(day!=26,0,if(status='H',9999,if(line=status, 1, 0)))) as fld28,
     sum(if(day!=27,0,if(status='H',9999,if(line=status, 1, 0)))) as fld29,
     sum(if(day!=28,0,if(status='H',9999,if(line=status, 1, 0)))) as fld30,
     sum(if(@numOfDays<29,9999, if(day!=29,0,if(status='H',9999,if(line=status, 1, 0))))) as fld31,
     sum(if(@numOfDays<30,9999, if(day!=30,0,if(status='H',9999,if(line=status, 1, 0))))) as fld32,
     sum(if(@numOfDays<31,9999, if(day!=31,0,if(status='H',9999,if(line=status, 1, 0))))) as fld33,
     '' as fld34, '' as fld35, '' as fld36
   from (
     select distinct
         cal.childID,
         atd.line ,
         day(cal.date) as day,
         if(cal.holiday , 'H' , if(a.attendAT >0, '蜃ｺ', '谺')) as status
	from (select s.date, c.id as childID, c.name, s.holiday
            from calendar s, 
                 child    c
			where s.date >= @1stDt and s.date <= @lastDT
			 and schoolAge(c.birthDay,s.date)=@schoolAge
          ) cal    
          left join 
          attending a  on cal.childID   = a.childID
                      and cal.date    = a.date
		  join atd
     ) data    
     group by data.line
     ) sum
	;
   
     drop temporary table atd;
   
   
END ;;
