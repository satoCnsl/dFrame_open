CREATE DEFINER=`root`@`localhost` PROCEDURE `setCaptionDic`()
BEGIN

  drop temporary table if exists tempCapDic;
  
  create temporary table tempCapDic
   select ordinaryWord, language, toString 
   from  dframe.__caption_dic;

  
  insert into tempCapDic  (ordinaryWord, language) 
  (select distinct ifnull(v.title, '') , ifnull(b.value, '' )
                                  from       __caption_list_v v
                                  inner join __base_value b
                                  left join  __caption_dic d
                                  on v.title = d.ordinaryWord 
                                  and b.value = d.language
    where b.catName='language' and d.language is null and v.title is not null)
    on duplicate key update ordinaryWord = '', language='';
    
    delete from dframe.__caption_dic;
    insert into __caption_dic (ordinaryWord, language, toString)
     (select distinct ordinaryWord, language, toString from tempCapDic order by language,ordinaryWord);
 
END ;;
