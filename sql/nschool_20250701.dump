CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryAttendSum`(_classDiaryID int, _dummy text)
BEGIN
  select month(d.date), 
         DATE_SUB(d.date, INTERVAL DAYOFMONTH(d.date)-1 DAY),LAST_DAY(d.date),
         DAYOFMONTH(LAST_DAY(d.date)), c.name, c.schoolAge
         into  @month, @1stDT, @lastDT, @numOfDays, @className, @schoolAge
    from nschool.classdiary d inner join nschool.nurseryclass c on c.id = d.classID
   where d.id = _classDiaryID;
   
   drop temporary table if exists atd;
   create temporary table atd as 
   select '蜃ｺ' as line union select '谺';
   
   select sum(if(c.sex=0 and c.admissionDate <=@1stDT and (c.graduationDate> @lastDT or c.graduationDate is null), 1 ,0)),
          sum(if(c.sex=1 and c.admissionDate <=@1stDT and (c.graduationDate> @lastDT or c.graduationDate is null), 1 ,0)),
          sum(if(c.sex=0 and c.admissionDate >=@1stDT and c.admissionDate <= @lastDT, 1 ,0)), 
          sum(if(c.sex=1 and c.admissionDate >=@1stDT and c.admissionDate <= @lastDT, 1 ,0)), 
          sum(if(c.sex=0 and c.graduationDate>=@1stDT and c.graduationDate<= @lastDT, 1 ,0)),
          sum(if(c.sex=1 and c.graduationDate>=@1stDT and c.graduationDate<= @lastDT, 1 ,0))
          into @numOfBoys, @numOfGirls, @newcommerBoys, @newcommerGirls, @graduateBoys , @graduateGirls
     from nschool.child c
	where nschool.schoolAge(c.birthDay,@lastDT)=@schoolAge;
    
    select
        concat('菫晁ご譌･謨ｰｼ', @numOfDays - f1 )                                      as fld1,
        concat('蜈･蝨呈焚縲逕ｷｼ',@newcommerBoys,'縲螂ｳｼ',@newcommerGirls )                as fld2,
        concat('騾蝨定縲逕ｷｼ',@graduateBoys, '縲螂ｳｼ',@graduateGirls )                 as fld3,
        concat('蝨ｨ邀肴焚縲逕ｷｼ',@numOfBoys,    '縲螂ｳｼ',@numOfGirls )                    as fld4, 
        concat('蜃ｺ蟶ｭ謨ｰ縲逕ｷｼ', f5b, '縲螂ｳｼ', f5g)                                     as fld5,
        concat('谺蟶ｭ謨ｰ縲逕ｷｼ', round((@numOfDays - f1)*@numOfBoys - f5b, 0), '縲螂ｳｼ',round((@numOfDays - f1)*@numOfGirls- f5g,0)) as fld6,
        concat('蜃ｺ蟶ｭ邱乗焚ｼ',  f5b + f5g)                                                   as fld7,
        concat('谺蟶ｭ邱乗焚ｼ',  round((@numOfDays - f1)*(@numOfBoys+@numOfGirls) - f5b -f5g,0))       as fld8
	  from (
        select 
           sum(if(line = '蜃ｺ' and status='莨',1,0))               as f1,
           sum(if(sex=0 and line=status and line = '蜃ｺ',1,0))   as f5b,
           sum(if(sex=1 and line=status and line = '蜃ｺ',1,0))   as f5g,
         
         
           sum(if(line=status and line = '蜃ｺ',1,0))               as f7
         
        from (   
           select distinct
                 c.id,
                 atd.line ,
                 c.sex,
                 day(s.date) day,
                 case when s.holiday then '莨' else
                    case when isnull(a.attendAT) then '谺' else '蜃ｺ' end 
		         end  status
	       from  atd,
                 calendar  s
	   left join attending a on s.date = a.date
       left join child     c on c.id   = a.childID
		                 and schoolAge(c.birthDay,s.date)=@schoolAge
           where s.date >= @1stDt and s.date <= @lastDT
        ) data
       ) sum 
   ;
     drop temporary table atd;
   
   
END ;;
DELIMITER ;
