CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryBabyRec`( _classDiaryID int,  _viewID int)
BEGIN
-- -----------------------------
--
--                  2025/01/28
-- ------------------------------
 DECLARE done          INT  DEFAULT FALSE;
 DECLARE _dt           date;
 DECLARE _med          text default '';  
 DECLARE _tempr        text default '';  
 DECLARE _mealVol      text default '';  
 DECLARE _milkVol      text default '';  
 DECLARE _nap          text default '';  
 DECLARE _feces        text default '';  
 DECLARE _bodyView     text default '';  
 DECLARE _cid          int  default 0;
 
 DECLARE curBD CURSOR FOR 
   select distinct
      cd.date dt,
      ch.id childID,
      case when isnull(b.bodyTemperatureHHMM) then '' 
        else concat('(' ,b.bodyTemperatureHHMM,')',b.bodyTemperature) end tempr,
      IFNULL(b.mealVol,'') mealVol,          
      case when isnull(b.milkVol)     then '' else concat(b.timeCategory,' ',b.milkVol,  '<p>') end milkVol,
	  case when isnull(b.napFrom)     then '' else concat(b.napFrom,     '-',b.napTo,    '<p>') end nap,
      case when isnull(b.fecesAt)     then '' else concat(b.fecesAt,     ' ',b.fecesVol, '<p>') end feces,
      case when isnull(b.bodyViewSel) then '' else concat(b.bodyViewSel, ' ',b.bodyView, '<p>') end bodyView
	from nschool.babydailyrec b
    inner join nschool.child   ch 
              on ch.id = b.childID 
	inner join nschool.classdiary cd  on cd.id = _classDiaryID
                                     and (cd.classID - 1) = nschool.schoolAge(ch.birthDay, cd.date)
	where b.date = cd.date
    order by ch.id; 
  
 DECLARE curM CURSOR FOR 
   select distinct
      ch.id childID,
      case when m.name is not null then 
         concat( '<p>',time_format(md.treatmentDateTime,'%H:%I') ,':',m.name, ' ', md.dose,'</p>')
         else ''
	  end med
	from nschool.medication    md
    inner join nschool.medicine m     on m.id = md.medicineID
    inner join nschool.child   ch     on ch.id = md.childID 
	inner join nschool.classdiary cd  on cd.id = _classDiaryID
                                     and (cd.classID - 1) = nschool.schoolAge(ch.birthDay, cd.date)
	where cast(treatmentDateTime as date) = cd.date
    order by ch.id; 

  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE;   
  
  -- select classID -1, date into @schoolAge, @date from nschool.classdiary where id = _classDiaryID; 
  
  drop temporary table if exists tempBD;  
  create temporary table tempBD( dt date, cid int, tempr varchar(256), mealVol varchar(256),  milkVol varchar(256),
                                      nap varchar(256), feces varchar(256), bodyView varchar(256));
  OPEN curBD;
    select 0, '', '', '', '', '', '', 0, '' into @dt, @tempr, @mealVol, @milkVol, @nap, @feces, @bodyView, @cid, @p ;
    read_loop: LOOP
    FETCH curBD INTO  _dt, _cid, _tempr, _mealVol, _milkVol, _nap, _feces, _bodyView ;    
      IF done THEN
        insert into tempBD values(_dt, @cid, @tempr, @mealVol, @milkVol, @nap, @feces, @bodyView);
        LEAVE read_loop;
      END IF;
      
      IF @cid = 0 then
        SET @cid = _cid;
        SET @p   = '<p>';
	  END IF;
     -- select @cid, _cid;
      IF @cid != _cid then
        insert into tempBD values(_dt, @cid, @tempr, @mealVol, @milkVol, @nap, @feces, @bodyView);
        set @cid = _cid;
        select '', '', '', '', '', '' into @tempr, @mealVol, @milkVol, @nap, @feces, @bodyView;        
	  END IF;
      
      set @tempr    = concat(@tempr,    @p, _tempr);
      set @mealVol  = concat(@mealVol,  @p, _mealVol);
      set @milkVol  = concat(@milkVol,  @p, _milkVol);
      set @nap      = concat(@nap,      @p, _nap);
      set @feces    = concat(@feces,    @p, _feces);
      set @bodyView = concat(@bodyView, @p, _bodyView);
      
        
    END LOOP;  
  CLOSE curBD;
  
 drop temporary table if exists tempMed;  
  create temporary table tempMed( cid int, med varchar(256));  
  
  OPEN curM;
    select '', 0 into @med, @cid ;
    read_loop: LOOP
    FETCH curM INTO  _cid, _med ;         
      IF @cid = 0 then
        SET @cid = _cid;
	  END IF;
      set @med = concat(@med , _med);
      IF @cid != _cid then
        insert into tempMed values(@cid, @med);
        set @cid = _cid;
	  END IF;
      IF done THEN
        LEAVE read_loop;
      END IF;
        
    END LOOP;  
  CLOSE curM;
  -- select * from tempBD;
  select "豌丞錐" AS fld1, "逋ｻ蝨呈凾蛻ｻ" AS fld2,"髯榊恍譎ょ綾" AS fld3,"讀懈ｸｩ" AS fld4,"鬟滉ｺ" AS fld5,"隱ｿ荵ｳ繝ｻ迚帑ｹｳ" AS fld6,
      "逹｡逵譎る俣" AS fld7,"謗剃ｾｿ" AS fld8,"霄ｫ菴捺園隕" AS fld9,"譛崎脈遲" AS fld10,"豢ｻ蜍輔ｻ縺昴ｮ莉" AS fld11,"蛯呵" AS fld12  
  union    select f1,f2,f3,f4,f5,f6,f7,f8,f9,f10,f11,f12 from
  (select
         concat(ch.familyName, ch.firstName) f1,  -- 豌丞錐
         a.attendAt                      f2,  -- 逋ｻ蝨呈凾蛻ｻ,
         a.leaveAt                       f3,  -- 髯榊恍譎ょ綾,         
         b.tempr                         f4,  -- 讀懈ｸｩ ,
         b.mealVol                       f5,  -- 鬟滉ｺ,           
         b.milkVol                       f6,  -- 隱ｿ荵ｳ繝ｻ迚帑ｹｳ
         b.nap                           f7,  -- 逹｡逵譎る俣,
         b.feces                         f8,  -- 謗剃ｾｿ,
         b.bodyView                      f9,  -- 霄ｫ菴捺園隕,
         m.med                           f10, -- 譛崎脈遲,
         concat(ifnull(cd.verbal, ''), ifnull(cd.humanRel, ''), ifnull(cd.health, ''),
         ifnull(cd.bodyMovement, ''), ifnull(cd.handMovement, '')) f11,   -- 豢ｻ蜍輔◎縺ｮ莉, 
         a.description                   f12   -- 蛯呵
    FROM       nschool.child        ch
    inner join tempBD               b  on ch.id   = b.cid
	inner join nschool.attending    a  on ch.id   = a.childID
                                      and isnull(a.attendAt) = false 
                                      and a.date  = b.dt
    left  join nschool.childdailyrec cd on ch.id   = cd.childID and cd.date = b.dt
	left  join tempMed   m  on ch.id   = m.cid
	order BY ch.id, a.id) q
    ;
END ;;
DELIMITER ;
