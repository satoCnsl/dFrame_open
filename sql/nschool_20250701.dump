         if(a.attendAT >0 , 1, 0) as atd,
         if(a.cause is not null, 1, 0) as cau,
         a.cause
	from (select seq.fld1, s.date, c.id as childID, concat(c.familyName,c.firstName) as name, s.holiday
            from calendar s,
                 child    c inner join seq on seq.childID = c.id
			where s.date >= @1stDt and s.date <= @lastDT
			 and schoolAge(c.birthDay,s.date)=@schoolAge
