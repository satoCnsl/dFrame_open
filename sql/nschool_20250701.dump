CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryFTR`(_id int, scope text)
BEGIN
   
   select ifnull(sd.handout, '(縺ｪ縺)') , cd.classID, ifnull(sk.snack1, '-'), ifnull(sk.snack2, '-')
         into @handout, @classID, @snack1, @snack2 
     from nschool.classdiary cd  left join nschool.schooldiary sd on sd.date = cd.date
								 left join nschool.snack     sk on sk.date = cd.date
	where cd.id = _id
    limit 1;
   
   set @lead = concat('<p>驟榊ｸ迚ｩｼ壹</p>', @handout);

   if @classID < 3 then
      select @lead as fld union select concat('<p>縺翫ｄ縺､ｼ壹</p>',@snack1,'<p>',@snack2,'</p>');
   else
      select @lead as fld;
   end if;
END ;;
