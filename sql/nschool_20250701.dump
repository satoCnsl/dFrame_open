CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepAttendList`(_cRprtID int)
BEGIN
  select month(d.date), 
         DATE_SUB(d.date, INTERVAL DAYOFMONTH(d.date)-1 DAY),LAST_DAY(d.date),
         DAYOFMONTH(LAST_DAY(d.date)), c.name, c.schoolAge
         into  @month, @1stDT, @lastDT, @numOfDays, @className, @schoolAge
    from nschool.classdiary d inner join nschool.nurseryclass c on c.id = d.classID
   where d.id = _cRprtID;
   
   select
     name,
     max(case when day = 1 then status else '' end) as d1,
     max(case when day = 2 then status else '' end) as d2,
     max(case when day = 3 then status else '' end) as d3,
     max(case when day = 4 then status else '' end) as d4,
     max(case when day = 5  then status else '' end) as d5,
     max(case when day = 6  then status else '' end) as d6,
