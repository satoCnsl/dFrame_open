CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryHDR`(_id int, scope text)
BEGIN
    select 0,    'No Data', 'No Data','No Data','        No Data',   'ｼ医↑縺暦ｼ', 0,             0 
    into   @day, @className, @weather, @roomTempelature, @staffName, @handout,  @attendantSum, @absenteeSum;

    select d.date, c.name, d.roomTemperature, s.name into  @day, @className, @roomTempelature, @staffName
            from nschool.classdiary d inner join nschool.staff s on d.staffID = s.id 
                                        inner join nschool.nurseryclass c on d.classID = c.id
            where d.id = _id;
            
    select sum(case when a.attendAt is not null then 1 else 0 end) ,
            sum(case when a.attendAt is null then 1 else 0  end) into @attendantSum, @absenteeSum
            from nschool.attending a inner join nschool.child c on a.childID = c.id          
            where a.date = @day group by a.date ;
    select weather, handout into @weather, @handout from nschool.schooldiary where date = @day limit 1;
    
    select CONCAT (@className, '縲' ,DATE_FORMAT(@day,"%c譛%e譌･"),
        "(", ELT(WEEKDAY(@day)+1,"譛","轣ｫ","豌ｴ","譛ｨ","驥","蝨","譌･"), ") "  ) AS  fld
    union  select CONCAT("蜃ｺ蟶ｭ", @attendantSum , "蜷")
