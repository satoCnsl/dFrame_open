CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryMedic`(_id int, scope text)
BEGIN
  DECLARE done INT DEFAULT FALSE;
  DECLARE _medic text;
  
  DECLARE cur1 CURSOR FOR 
   select distinct 
         concat(c.name, '(', m.name,'縲',mc.dose, ')' )  as medic
        from nschool.medication mc inner join nschool.child c on mc.childID = c.id
                                  inner join nschool.medicine m on mc.medicineID = m.id
                                  inner join nschool.classDiary d on d.date = cast(mc.treatmentDatetime AS DATE)
                                                                    and d.id = _id;
  
  DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE; 
  set @medic = '<p>譁ｽ阮ｬｼ</p>';
  set @i = 0;
  OPEN cur1;
  read_loop1: LOOP
     IF done THEN
       LEAVE read_loop1;
     END IF;
     FETCH cur1 INTO _medic;
     if _medic != 'NULL' and done = false then
        set @medic = concat( @medic, '縲', _medic); 
        set @i = @i +1;
	 end if;
     	
  END LOOP;  
  CLOSE cur1;  
  
  select @medic as fld;
END ;;
