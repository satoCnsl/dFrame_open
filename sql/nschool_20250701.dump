CREATE DEFINER=`root`@`localhost` FUNCTION `schoolAge`(_birthday date, _date date) RETURNS text CHARSET utf8mb3
    DETERMINISTIC
BEGIN

  
  
  select IF(month(_date) <4 , year(_date) - 1 , year(_date)) into @year;
  
  
  if month(_birthday) = 4 and day(_birthday) = 1 then
    select  year(_birthday) - 1 into @byear;
  else
    select  IF(month(_birthday) <4 , year(_birthday) -1 , year(_birthday)) into @byear;
  end if;
  
  
  if (@year = @byear ) then
    return 0;
  else
	RETURN (@year - @byear- 1);
  end if;
END ;;
