CREATE DEFINER=`root`@`localhost` PROCEDURE `getRepClassDiaryAttendList`(_classDiaryID int, _dummy text)
BEGIN
-- -------------------------
-- 
--                          2025/01/26 name ==>concat(familyName, firstName)
-- ------------------------
  select month(d.date), 
         DATE_SUB(d.date, INTERVAL DAYOFMONTH(d.date)-1 DAY),LAST_DAY(d.date),
         DAYOFMONTH(LAST_DAY(d.date)), c.name, c.schoolAge
         into  @month, @1stDT, @lastDT, @numOfDays, @className, @schoolAge
    from nschool.classdiary d inner join nschool.nurseryclass c on c.id = d.classID
   where d.id = _classDiaryID;
   
   drop temporary table if exists atd;
   create temporary table atd as 
   select '蜃ｺ' as line union select '谺';
   
   
   drop temporary table if exists seq;
   create temporary table seq (
      `fld1` int auto_increment,
      childID int,
      PRIMARY KEY (`fld1`)
	); 

   insert into seq select NULL, id as childID from nschool.child where schoolAge(birthDay,@lastDT)=@schoolAge;

   select 'No' as fld1, '豌丞錐' as fld2,
      1 as fld3,  2 as  fld4,  3 as  fld5,  4 as  fld6,  5 as  fld7,  6 as  fld8,  7 as  fld9,  8 as  fld10,  9 as fld11, 10 as fld12, 
     11 as fld13, 12 as fld14, 13 as fld15, 14 as fld16, 15 as fld17, 16 as fld18, 17 as fld19, 18 as fld20, 19 as fld21, 20 as fld22, 
     21 as fld23, 22 as fld24, 23 as fld25, 24 as fld26, 25 as fld27, 26 as fld28, 27 as fld29, 28 as fld30,
     if(@numOfDays < 29 , '' , 29 ) as fld31,
