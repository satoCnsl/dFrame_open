     max(case when day = 20  then status else '' end) as d20,
     max(case when day = 21 then status else '' end) as d21,
     max(case when day = 22  then status else '' end) as d22,
     max(case when day = 23  then status else '' end) as d23,
     max(case when day = 24  then status else '' end) as d24,
     max(case when day = 25  then status else '' end) as d25,
     max(case when day = 26  then status else '' end) as d26,
     max(case when day = 27  then status else '' end) as d27,
     max(case when day = 28  then status else '' end) as d28,
     max(case when day = 29  then status else '' end) as d29,
     max(case when day = 30  then status else '' end) as d30,
     max(case when day = 31  then status else '' end) as d31,
     sum(atd)                                         as atd,
     @numOfDays - sum(atd) - sum(cau)				  as abs,
     sum(cau)                                         as cau,
     max(cause)                                       as cause
   from (
     select distinct
         day(cal.date) day,
         cal.name,
         cal.childID,
         case when cal.holiday then '' else
           case when a.attendAT >0 then '蜃ｺ' else '谺' end 
		 end as status,
         case when a.attendAT >0 then 1 else 0 end as atd,
         case when a.cause is not null then 1 else 0 end as cau,
         a.cause
	from (select s.date, c.id as childID, c.name, s.holiday
            from calendar s, 
                 child    c
			where s.date >= @1stDt and s.date <= @lastDT
			  and schoolAge(c.birthDay,s.date)=@schoolAge
          ) cal    
          left join 
          attending a  on cal.childID   = a.childID
                           and cal.date    = a.date
     ) data    
    group by name, childID
	order by childID
        ;
   
   
   
   
   
END ;;
